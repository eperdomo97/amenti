# Temp files
/tmp/*
!/tmp/cache/models/empty
!/tmp/cache/persistent/empty
!/tmp/cache/views/empty
!/tmp/sessions/empty
!/tmp/tests/empty

# Operating system generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# MediaWiki specific directories to exclude
# Uploaded files
/htdocs/mywiki/images/

# Cached files
/htdocs/mywiki/cache/
/htdocs/mywiki/sessions/
/htdocs/mywiki/languages/cache/

# Compiled assets
/htdocs/mywiki/resources/assets/

# LocalSettings and other configuration files
/htdocs/mywiki/LocalSettings.php
/htdocs/mywiki/AdminSettings.php

# CakePHP specific
/vendor/*
/config/app.php

/app/tmp/*
/app/Config/core.php
/app/Config/database.php
/vendors/*

